{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "GDB Debug",
      "type": "cppdbg",
      "request": "launch",
      "cwd": "${cmake.testWorkingDirectory}",
      "program": "${cmake.testProgram}",
      "args": [
        "${cmake.testArgs}"
      ],
      "setupCommands": [
        {
          "text": "-enable-pretty-printing",
        }
      ]
    }
  ]
}